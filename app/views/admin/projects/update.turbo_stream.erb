<%= turbo_stream.replace dom_id(@project) do %>
  <%= render "admin/projects/project", project: @project %>
<% end %>

<%= turbo_stream.update "modal" do %>
  <div class="bg-white px-4 pt-5 pb-4 sm:p-6 sm:pb-4">
    <div class="sm:flex sm:items-start">
      <div class="mt-3 text-center sm:mt-0 sm:text-left w-full">
        <h3 class="text-lg leading-6 font-medium text-gray-900 mb-4">Edit Project</h3>
        <%= render "form", agency: @agency, project: @project %>
      </div>
    </div>
  </div>
<% end %>